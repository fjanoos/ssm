@ARTICLE{Roche2011,
  author = {A. Roche},
  title = {A four-dimensional registration algorithm with application to joint
	correction of motion and slice timing in fMRI.},
  journal = {IEEE Trans Med Imaging},
  year = {2011},
  month = {Mar},
  abstract = {Existing groupwise image registration algorithms for longitudinal
	data generally ignore continuous movements and signal changes that
	occur throughout image acquisition. We emphasize the case of functional
	magnetic resonance images, which present spatio-temporal distortion
	due to the combination of head motion during scanning and staggered
	slice acquisition. While there exist techniques to correct for motion
	and slice timing separately, a common dilemma is to determine which
	correction should be applied first. This paper proposes a fourdimensional
	realignment algorithm to perform both tasks simultaneously. Experiments
